#include<iostream>
using namespace std;

int main()
{
	double a, b, c, d, e;
	cout << "Nhap 5 so thuc " << endl;
	cin >> a >> b >> c >> d >> e;
	double max = a;
	if (b > max) {
		max = b;
	}
	if (c > max) {
		max = c;
	}
	if (d > max) {
		max = d;
	}
	if (e > max) {
		max = e;
	}
	if ( a == b && b == c && c == d && d == e) {
		cout << "Khong co so lon nhat" << endl;
	}
	else {
		cout << "So lon nhat trong 5 so la " << max << endl;
	}
	return 0;
}
