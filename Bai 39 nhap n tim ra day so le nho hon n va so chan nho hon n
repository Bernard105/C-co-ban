#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
	int n;
	cout << "Nhap so " << endl;
	cin >> n;

	cout << "Day so chan nho hon " << n << endl;

	for (int i = 2; i < n; i += 2)
	{
		cout << setw(5) << i;
	}
	cout << endl;

	cout << "Day so le nho hon " << n << endl;

	for (int j = 1; j < n; j += 2)
	{
		cout << setw(5) << j;
	}
	cout << endl;
	return 0;
}
