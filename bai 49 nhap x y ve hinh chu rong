#include<iostream>
using namespace std;

int main()
{
	int x, y;
	cout << "Enter numbers" << endl;
	cin >> x >> y;

	for (int i = 1; i <= y; i++) {
		for (int j = 1; j <= x; j++)
		{
			if (i == 1 || i == y || j == 1 || j == x)
			{
				cout << "*";
			}
			else
			{
				cout << " ";
			}
		}
		cout << endl;
	}
}
