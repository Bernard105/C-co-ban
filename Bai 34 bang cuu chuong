#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
	cout << "Bang cuu chuong " << endl;

	for (int i = 1; i <= 10; i++) { 
		cout << setw(18) << "Bang nhan " << i << endl;
		for (int j = 1; j <= 10; j++) {
			cout << setw(15) << i << " * " << j << " = " << i * j << endl;
		}
	}
}
